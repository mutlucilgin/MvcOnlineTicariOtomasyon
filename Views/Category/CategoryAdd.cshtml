@model MvcOnlineTicariOtomasyon.Models.Siniflar.Category
@{
    ViewBag.Title = "CategoryAdd";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>Yeni Kategori Sayfası</h2>
<br />

<form class="form-group" method="post">
    @Html.LabelFor(x=>x.CategoryName)
    @Html.TextBoxFor(x=>x.CategoryName,new {@class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

